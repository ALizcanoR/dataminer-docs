{
  "build": {
    "content": [
      {
        "files": [
          "user-guide/**.md",
          "user-guide/**/toc.yml",
          "develop/**.md",
          "develop/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "user-guide/images/**",
          "develop/images/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "DataMiner Docs",
      "_appFooter": "Â© Skyline Communications 2022",
      "_enableSearch": true,
      "_enableNewTab": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/slc_template"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
